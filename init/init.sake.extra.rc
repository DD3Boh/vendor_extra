on property:vendor.post_boot.parsed=1

	# Set Gold and Gold+ max freqs
	write /sys/devices/system/cpu/cpufreq/policy4/scaling_max_freq 1996800
	write /sys/devices/system/cpu/cpufreq/policy7/scaling_max_freq 1900800

	# Set power efficient kernel scheduling
	write /proc/sys/kernel/sched_downmigrate "75 75"
	write /proc/sys/kernel/sched_group_upmigrate 115
	write /proc/sys/kernel/sched_group_downmigrate 75
	write /proc/sys/kernel/sched_coloc_busy_hyst_cpu_ns "39000000 39000000 39000000 39000000 10000000 10000000 10000000 5000000"
	write /proc/sys/kernel/sched_coloc_busy_hyst_cpu_busy_pct "10 10 10 10 30 30 30 95"
	write /proc/sys/kernel/sched_min_task_util_for_boost 65
	write /proc/sys/kernel/sched_min_task_util_for_colocation 25

	# Gold cores core_ctl
	write /sys/devices/system/cpu/cpu4/core_ctl/min_cpus 0

	# Gold+ core_ctl
	write /sys/devices/system/cpu/cpu7/core_ctl/busy_down_thres 40
	write /sys/devices/system/cpu/cpu7/core_ctl/offline_delay_ms 50
	write /sys/devices/system/cpu/cpu7/core_ctl/task_thres 3
	write /sys/devices/system/cpu/cpu7/core_ctl/not_preferred 1
